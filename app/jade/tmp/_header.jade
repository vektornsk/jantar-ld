mixin header()
	.header&attributes(attributes)
		.logo
			a.logo__link(href='#')
				img.logo__img(src="/images/icons/logo.svg", title="Янтарь", alt="Янтарь")
		.tel
			img.tel__icon(src='/images/icons/phone.svg')
			span Телефоны автомоек:
			ul.tel-list
				li.tel-list__item ул.Большевистская, 52
					div + 7 (903) 903-95-52
				li.tel-list__item Проезд Энергетиков, 1/1
					div + 7 (901) 451-77-99
				li.tel-list__item ул. Военная, 8а
					div + 7 (383) 209-22-82
					
					
